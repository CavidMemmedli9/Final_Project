@model FooterVM

<footer class="site-footer footer-light">

    <!-- FOOTER NEWSLETTER START -->
    <div class="footer-top-newsletter">
        <div class="container">
            <div class="sf-news-letter">
                <span>Subscribe Our Newsletter</span>
                <form asp-action="Subscribes" asp-controller="Home" method="post">

                    <div class="form-group sf-news-l-form">
                        <input type="text" class="form-control" name="email" placeholder="Enter Your Email">
                        <button type="submit" class="sf-sb-btn">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- FOOTER BLOCKES START -->
    <div class="footer-top">
        <div class="container">
            <div class="row">
                <!-- Footer col 1-->
                <div class="col-lg-3 col-md-6 col-sm-6  m-b30">
                    <div class="sf-site-link sf-widget-link">
                        <h4 class="sf-f-title">Site Links</h4>
                        <ul>
                            <li><a asp-controller="home" asp-action="index">Home</a></li>
                            <li><a asp-controller="about" asp-action="index">About Us</a></li>
                            <li><a asp-controller="category" asp-action="index">Categories</a></li>
                            <li><a asp-controller="profile" asp-action="index">Profile</a></li>
                            <li><a asp-controller="job" asp-action="index">Job</a></li>
                            <li><a asp-controller="blog" asp-action="index">Blog</a></li>
                            <li><a asp-controller="contact" asp-action="index">Contact</a></li>
                        </ul>
                    </div>
                </div>
                <!-- Footer col 2-->
                <div class="col-lg-3 col-md-6 col-sm-6  m-b30">
                    <div class="sf-site-link sf-widget-cities">
                        <h4 class="sf-f-title">Popular Cities</h4>
                        <ul>
                            @foreach (var item in Model.City)
                            {
                                <li><a href="all-categories.html">@item.Name</a></li>
                            }

                           

                        </ul>
                    </div>
                </div>
                <!-- Footer col 1-->
                <div class="col-lg-3 col-md-6 col-sm-6  m-b30">
                    <div class="sf-site-link sf-widget-categories">
                        <h4 class="sf-f-title">Categories</h4>
                        <ul>
                            @foreach (var item in Model.Category)
                            {

                                <li><a href="/Job?s=&categoryid=@item.Id">@item.Name</a></li>
                            }
                        </ul>
                    </div>
                </div>
                <!-- Footer col 1-->
                <div class="col-lg-3 col-md-6 col-sm-6  m-b30">
                    <div class="sf-site-link sf-widget-contact">
                        <h4 class="sf-f-title">Contact Info</h4>
                        <ul>
                            @foreach (var item in Model.Settings)
                            {

                                <li>@item.Place</li>

                                <li>@item.Number</li>

                                <li>@item.EmailAddress</li>


                            }
                        </ul>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <!-- FOOTER COPYRIGHT -->
    <div class="footer-bottom">
        <div class="container">
            <div class="sf-footer-bottom-section">
                <div class="sf-f-logo"><a href="javascript:void(0);"><img src="~/assets/images/logo-dark.png" alt=""></a></div>
                <div class="sf-f-copyright">
                    <span>Copyright 2022 | Aone Theme. All Rights Reserved</span>
                </div>

                <div class="sf-f-social">
                    <ul class="socila-box">
                        <li><a href="https://twitter.com/"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="https://www.facebook.com/"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="https://www.linkedin.com/"><i class="fa fa-linkedin"></i></a></li>
                        <li><a href="https://www.instagram.com/"><i class="fa fa-instagram"></i></a></li>
                    </ul>
                </div>

            </div>
        </div>
    </div>

</footer>
<!-- FOOTER END -->
