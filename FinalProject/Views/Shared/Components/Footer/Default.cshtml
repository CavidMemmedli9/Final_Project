<script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.3.4/axios.min.js" integrity="sha512-LUKzDoJKOLqnxGWWIBM4lzRBlxcva2ZTztO8bTcWPmDSpkErWx0bSP4pdsjNH8kiHAUPaT06UXcb+vOEZH+HpQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script>
    $("#loginformBtn").on("click", function () {
        var email = $("#Email").val();
        var password = $("#Password").val();
        var bodyFormData = new FormData();

        bodyFormData.append('Email', email);
        bodyFormData.append('Password', password);
        bodyFormData.append('RememberMe', false);

        axios({
            method: "post",
            url: "/Account/Login/",
            data: bodyFormData,
            headers: { "Content-Type": "multipart/form-data" },
        }).then(function (response) {
            if(response.data == "1"){
                $(location).attr('href', '/Home');
            }
            $("#errormessagesLogin").text(response.data)

            console.log(response);
            }).catch(function (response) {
                console.log(response);
            });
    })


    $("#RegisterBtn").on("click", function () {
        var FullName1 = $("#FullName1").val();
        var UserName1 = $("#UserName1").val();
        var email1 = $("#Email1").val();
        var password1 = $("#Password1").val();
        var ConfirmPassword1 = $("#PasswordConfirm1").val();
        var bodyFormData = new FormData();

        bodyFormData.append('Email', email1);
        bodyFormData.append('Password', password1);
        bodyFormData.append('FullName', FullName1);
        bodyFormData.append('UserName', UserName1);
        bodyFormData.append('PasswordConfirm', ConfirmPassword1);

        //bodyFormData.append('RememberMe', false);

        axios({
            method: "post",
            url: "/Account/Register/",
            data: bodyFormData,
            headers: { "Content-Type": "multipart/form-data" },
        }).then(function (response)
        {
            console.log(response.data)
            if (response.data == "1") {
                $(location).attr('href', '/Home');
            }
            $("#errormessagesRegister").text(response.data)

            console.log(response);
        }).catch(function (response) {
            console.log(response);
        });
    })

   


</script>
@if (TempData["Error"] != null)
{
