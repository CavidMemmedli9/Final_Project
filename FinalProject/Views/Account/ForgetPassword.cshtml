
@model ForgetPasswordVM

@{
    ViewData["Title"] = "ForgetPassword";
}

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.2.3/css/bootstrap.min.css" integrity="sha512-SbiR/eusphKoMVVXysTKG/7VseWii+Y3FdHrt0EpKgpToZeemhqHeZeLWLhJutz/2ut2Vw1uQEj2MbRF+TVBUA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
<div  class="row mt-3">
    <h1 style="  margin-top:90px;margin-left:50px">Forget Password</h1>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <form method="post">
        <div style="margin-left:50px" class="mt-3">
            <label asp-for="AppUser.Email">Pls Enter Your Email</label>
            <input style="width:200px" asp-for="AppUser.Email" class="form-control mt-3" /> <br />

            <span class="text-danger">
                @Html.ValidationMessage("Error1")
            </span> <br /> <br />
            <button class="btn btn-outline-dark" type="submit">Send</button>
        </div>
    </form>
</div>

