@model ForgetPasswordVM

@{
    ViewData["Title"] = "ResetPassword";
}

<div class="container mt-3">
    <form  method="post">
        <div asp-validation-summary="All"></div>
        <div  class="mt-3">
            <label style="margin-left:50px;margin-top:70px" asp-for="Password">Password:</label>
            <input asp-for="Password" class="form-control" />
        </div>
        <div class="mt-3">
            <label style="margin-left:50px" asp-for="ConfirmPassword">Password:</label>
            <input asp-for="ConfirmPassword" class="form-control" />
        </div>
        <button style="margin-top:20px" type="submit" class="btn btn-outline-dark">Reset Password</button>
    </form>
</div>

